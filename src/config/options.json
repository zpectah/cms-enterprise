{
  "model": {
    "Categories": {
      "type": ["posts", "products"]
    },
    "Menu": {
      "type": ["primary", "secondary", "tertiary", "custom"]
    },
    "MenuItems": {
      "type": ["page", "local", "external"]
    },
    "Pages": {
      "type": ["default", "system", "category"]
    },
    "Posts": {
      "type": ["article", "blog", "media", "gallery", "event", "reference", "testimonial"]
    },
    "Tags": {
      "type": ["default"]
    },
    "Translations": {
      "type": ["default"]
    },
    "Uploads": {
      "type": ["unknown", "image", "audio", "video", "document", "archive"],
      "audio": {
        "extension": ["mp3", "aac"]
      },
      "video": {
        "extension": ["mpeg", "mp4"]
      },
      "document": {
        "extension": ["doc", "docs", "xls", "xlsx", "ppt", "pptx", "pps", "ppsx", "pages", "numbers", "pdf"]
      },
      "archive": {
        "extension": ["zip", "rar"]
      },
      "image": {
        "extension": ["jpg", "jpeg", "png"],
        "cropped": {
          "key": "cropped",
          "width": null,
          "height": null,
          "quality": 75,
          "crop": true
        },
        "format": [
          {
            "key": "thumbnail",
            "width": 100,
            "height": 100,
            "quality": 50,
            "crop": true
          },
          {
            "key": "medium",
            "width": 1280,
            "height": 720,
            "quality": 80,
            "crop": true
          },
          {
            "key": "large",
            "width": 1920,
            "height": 1080,
            "quality": 100,
            "crop": false
          },
          {
            "key": "custom_1",
            "width": 1920,
            "height": 640,
            "quality": 75,
            "crop": true
          }
        ]
      }
    },
    "Users": {
      "type": ["test", "default"],
      "level": ["redactor", "chief_redactor", "admin", "super_admin"],
      "group": ["company", "redaction"]
    },
    "Members": {
      "type": ["default", "customer", "subscriber", "hidden"]
    },
    "Deliveries": {
      "type": ["default"]
    },
    "Distributors": {
      "type": ["default"]
    },
    "Orders": {
      "type": ["default"],
      "status": [0,1,2,3]
    },
    "Payments": {
      "type": ["default"]
    },
    "Producers": {
      "type": ["default"]
    },
    "Products": {
      "type": ["default", "licence", "service"]
    },
    "ProductsOptions": {
      "type": ["default", "custom_value"]
    },
    "Stores": {
      "type": ["default", "virtual", "branch"]
    }
  },
  "module": {},
  "component": {},
  "common": {
    "meta_robots": [
      "all",
      "none",
      "noindex",
      "nofollow",
      "index",
      "follow",
      "nosnippet",
      "noarchive"
    ]
  }
}